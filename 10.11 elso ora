    print('Melyik a legnagyobb')

x = int(input('Első'))
y = int(input('Második')) 
z = int(input('Harmadik'))
if y < x > z : 
    print('Első a nagyobb')

elif z < y > x : 
    print('Második a nagyobb')

elif x < z > y : 
    print('Harmadik a nagyobb')

else:
    print('Irj mást')

    





    print('Melyik a legkissebb')
    
    x = int(input('Első'))
y = int(input('Második')) 
z = int(input('Harmadik'))
if y > x < z : 
    print('Első a kisebb')

elif z > y < x : 
    print('Második a kisebb')

elif x > z < y : 
    print('Harmadik a kisebb')

else:
    print('Irj mást')



    nap = input('Órarend (h,k,sz,cs,p):')
    if nap == 'h':
        print('1.óra:MATEK')
        print('2.óra:TESI')


    elif nap == 'k':
        print('1.óra:KÖRNYEZET')
        print('2.óra:TESI')

        elif nap == 'sz':
        print('1.óra:MATEK')
        print('2.óra:KÖRNYEZET')

        elif nap == 'cs':
        print('1.óra:KÖRNYEZET')
        print('2.óra:MAGYAR')

        elif nap == 'p':
        print('1.óra:KÖRNYEZET')
        print('2.óra:MATEK')

        else:
            print('Nem jó a válaszod')
