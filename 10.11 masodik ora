
 nap = input('Órarend (h,k,sz,cs,p):')
    if nap == 'h':
        print('1.óra:MATEK')
        print('2.óra:TESI')


    elif nap == 'k':
        print('1.óra:KÖRNYEZET')
        print('2.óra:TESI')

        elif nap == 'sz':
        print('1.óra:MATEK')
        print('2.óra:KÖRNYEZET')

        elif nap == 'cs':
        print('1.óra:KÖRNYEZET')
        print('2.óra:MAGYAR')

        elif nap == 'p':
        print('1.óra:KÖRNYEZET')
        print('2.óra:MATEK')

        else:
            print('Nem jó a válaszod')
